iframe{
	border: none;
}
#comment{
	margin:20px 20px;
}

input{
	color:white;
	font-size:17px;
	width:100px;
	height: 30px;
	border:none;
	background-color:black;
	cursor: pointer;
	outline:0;
	font-family:dqfont;
	padding: 0px;
}
input:hover{
 	color:orange;
}
table{
	margin: 20px 0px 0px 0px;
}
.text table tr th{
	width: 120px;
	height: 30px;
	padding:0px;
	}

.text button{
	font-family:dqfont;
	  width		 	: 120px;
  display       : inline-block;
  border-radius : 9%;
  font-size     : 10pt;
  text-align    : center;      /* 文字位置   */
  cursor        : pointer;     /* カーソル   */
  padding       : 10px 15px;   /* 余白       */
  background    : rgba(0, 0, 0, 0.10);     /* 背景色     */
  color         : #ffffff;     /* 文字色     */
  line-height   : 0.3em;         /* 1行の高さ  */
  opacity       : 0.9;         /* 透明度     */
  transition    : .3s;         /* なめらか変化 */
}
}
