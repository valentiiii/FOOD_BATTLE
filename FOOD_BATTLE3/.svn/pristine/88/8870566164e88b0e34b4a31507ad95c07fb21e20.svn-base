.outline{
	margin:40px 0px 0px 0px;
	width: auto;
    height: 250px;
      display: -webkit-flex;
  display: flex;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-align-items: stretch;
  align-items: stretch;
}

/* ------------------------------------------------ */
.comand{
	width:300px;
	height: 200px;
	margin:20px 0px 20px 20px;
	border: 3px solid white ;
	border-radius:10px;
	float:left;
	text-align: center;

}

.comand ul{
	margin:15px 0px 0px 0px;
	padding: 0px;
}

.comand li{
	margin:10px 0px 10px 0px;
	list-style: none;
}
.comand input{
	color:white;
	font-size:25px;
	width:120px;
	height: 30px;
	border:none;
	background-color:black;
	cursor: pointer;
	outline:0;
	font-family:dqfont;

}
.comand input:hover{
 color:orange;
}
